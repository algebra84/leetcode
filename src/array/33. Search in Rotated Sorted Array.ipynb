    "def search(nums, target):\n",
    "    \"\"\"\n",
    "    :type nums: List[int]\n",
    "    :type target: int\n",
    "    :rtype: int\n",
    "    \"\"\"\n",
    "    if len(nums) == 0:\n",
    "        return -1\n",
    "    p = 0\n",
    "    while(p < len(nums) and nums[p] != target):\n",
    "        p+=1\n",
    "    if p == len(nums):\n",
    "        return -1\n",
    "    else:\n",
    "        return p"
